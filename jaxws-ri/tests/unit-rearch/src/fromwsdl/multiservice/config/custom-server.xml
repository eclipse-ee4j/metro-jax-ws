<bindings 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    wsdlLocation="PingService.wsdl"
    xmlns="http://java.sun.com/xml/ns/jaxws">    
    <bindings node="wsdl:definitions" >
        <package name="fromwsdl.multiservice.server"/>
	<enableWrapperStyle>false</enableWrapperStyle>
    </bindings>
    <bindings node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://xmlsoap.org/Ping']">
        <jaxb:schemaBindings>
            <jaxb:package name="fromwsdl.multiservice.server"/>
        </jaxb:schemaBindings>
    </bindings>
</bindings>

