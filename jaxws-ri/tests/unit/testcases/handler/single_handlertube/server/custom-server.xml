<bindings xmlns="https://jakarta.ee/xml/ns/jaxws">

  <bindings>
    <handler-chains xmlns="https://jakarta.ee/xml/ns/jakartaee">

     <handler-chain>
        <port-name-pattern xmlns:ns1="urn:test">ns1:HelloPort</port-name-pattern>
        <handler>
          <handler-name>RandomName</handler-name>
          <handler-class>handler.single_handlertube.common.ServerSoapHandler</handler-class>
        </handler>
      </handler-chain>
      
      <handler-chain>
        <port-name-pattern xmlns:ns1="urn:test">ns1:HelloPort12</port-name-pattern>
        <handler>
          <handler-class>handler.single_handlertube.common.ServerLogicalHandler</handler-class>
        </handler>
      </handler-chain>
      
    </handler-chains>
  </bindings>

</bindings>

