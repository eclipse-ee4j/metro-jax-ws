<bindings xmlns="https://jakarta.ee/xml/ns/jaxws">
  
  <bindings xmlns:jws="https://jakarta.ee/xml/ns/jakartaee">
    <jws:handler-chains>
    
      <jws:handler-chain>
        <jws:protocol-bindings>##SOAP11_HTTP</jws:protocol-bindings>
        <jws:handler>
          <jws:handler-class>fromwsdl.handler_simple_rpclit.common.SOAPTestHandler</jws:handler-class>
        </jws:handler>
      </jws:handler-chain>
      
      <jws:handler-chain>
        <jws:protocol-bindings>##SOAP12_HTTP</jws:protocol-bindings>
        <jws:handler>
          <jws:handler-class>fromwsdl.handler_simple_rpclit.client.Port12Handler</jws:handler-class>
        </jws:handler>
      </jws:handler-chain>
      
    </jws:handler-chains>
  </bindings>
  
</bindings>
